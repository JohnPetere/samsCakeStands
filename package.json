{
  "name": "cake-stand-sam",
  "version": "1.0.0",
  "description": "### Initialize Project Folder to Use NPM  - on the command line run the following command. ```npm    npm init ```",
  "scripts": {
    "clean": "rm -rf node_modules .git .parcel-cache dist .DS_Store",
    "static": "cp -rf ./src/static   dist",
    "start": "npx parcel src/index.html ",
    "build": "npx parcel build src/index.html",
    "build:css": "tailwind build src/css/style.css -o dist/style.css",
    "buildtwcss": "npx tailwindcss -i ./src/css/styles.css -o ./dist/styles.css --watch"
  },
  "keywords": [],
  "author": "John-Peter Eberhard",
  "license": "ISC",
  "dependencies": {
    "base-64": "^1.0.0",
   
    "firebase": "^9.6.5",
    "nanoid": "^3.2.0",
    "parcel": "^2.2.1"
  },
  "devDependencies": {
    "@parcel/packager-raw-url": "^2.2.1",
    "@parcel/transformer-webmanifest": "^2.2.1",
    "tailwindcss": "^3.0.22"
  }
}
